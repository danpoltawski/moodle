<ul class="treeview" id="tree-view-{{uniqid}}">
    <li role="treeitem">This is an item which has children
        <ul role="group">
            <li role="treeitem">A child</li>
            <li role="treeitem">Another child</li>
        </ul>
    </li>
</ul>

